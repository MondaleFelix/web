<h1>Results Page!!</h1>


<!-- Loops through movie results and places title and year into individual li -->
<ul>
<% data["Search"].forEach(function(movie){ %>
  <li>
    <strong><%= movie["Title"] %></strong> - <%= movie["Year"]%>
  </li>
<% })%>
</ul>

<!-- returns to the search page -->
<a href="/">Search again</a>
